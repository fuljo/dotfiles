[Desktop Entry]
Name=WhatsApp
Comment=Whatsapp Web through Chrome
Exec=/usr/bin/google-chrome-stable --app="https://web.whatsapp.com" %U
Icon=whatsapp
Terminal=false
StartupWMClass=whatsapp
Type=Application
Categories=Chat;Network;InstantMessaging;
MimeType=x-scheme-handler/whatsapp;
Keywords=whatsapp;chat;im;messaging;messenger;sms;whatsappweb;
SingleMainWindow=true
X-GNOME-UsesNotifications=true
X-GNOME-SingleWindow=true

